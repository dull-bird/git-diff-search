{
  "name": "git-diff-search",
  "displayName": "Git Diff Search",
  "description": "%extension.description%",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Source Control",
    "Search"
  ],
  "activationEvents": [
    "onView:gitDiffSearch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "scm": [
        {
          "type": "webview",
          "id": "gitDiffSearch",
          "name": "%view.name%",
          "visibility": "visible"
        }
      ]
    },
    "commands": [
      {
        "command": "gitDiffSearch.focus",
        "title": "%command.focus.title%",
        "icon": "$(search)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "gitDiffSearch.focus",
          "when": "view == scm",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
}
